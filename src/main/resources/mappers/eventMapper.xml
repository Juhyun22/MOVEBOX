<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.movebox.event.dao.EventDAO">
	<select id="selectEventList" resultType="com.movebox.event.model.Event">
		SELECT 
			`id`
			, `userId`
			, `writter`
			, `title`
			, `imagePath`
			, `content`
			, `createdAt`
			, `updatedAt`
		FROM 
			`event`
	</select>
</mapper>